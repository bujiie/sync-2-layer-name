{
    "compilerOptions": {
        "outDir": "dist/",
        "noImplicitAny": false,
        "module": "es6",
        "target": "es5",
        "allowJs": true,
        "moduleResolution": "node",
        "lib": [
            "es6"
        ],
        "typeRoots": [
            "./node_modules/@types",
            "./node_modules/@figma"
        ],
        // allows using iterator
        "downlevelIteration": true
    },
}